{
  "name": "@enfexia/carbone",
  "description": "Fast, Simple and Powerful report generator. Injects JSON and produces PDF, DOCX, XLSX, ODT, PPTX, ODS, ...!",
  "version": "0.0.1",
  "bin": {
    "carbone": "bin/carbone"
  },
  "main": "./lib",
  "author": "David Grelaud",
  "license": "Apache-2.0",
  "homepage": "https://carbone.io",
  "keywords": [
    "report",
    "document",
    "pdf",
    "xslx",
    "docx",
    "odt",
    "ods",
    "json"
  ],
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha test --timeout 100000 --exit",
    "lint": "eslint ./lib/**.js ./test/**.js ./formatters/**.js",
    "lint:fix": "eslint ./lib/**.js ./test/**.js ./formatters/**.js --fix"
  },
  "dependencies": {
    "dayjs": "=1.10.4",
    "debug": "=4.1.1",
    "image-size": "^0.9.3",
    "qrcode": "^1.4.4",
    "timsort": "=0.3.0",
    "which": "=2.0.2",
    "@enfexia/xml2js": "=0.0.1",
    "xml2js-xpath": "^0.11.0",
    "yauzl": "=2.10.0",
    "yazl": "=2.5.1"
  },
  "devDependencies": {
    "eslint": "=7.7.0",
    "mocha": "=8.1.3"
  },
  "engines": {
    "node": ">= 10.15.0"
  },
  "directories": {
    "doc": "doc",
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Enfexia/carbone.git"
  },
  "bugs": {
    "url": "https://github.com/Enfexia/carbone/issues"
  }
}
